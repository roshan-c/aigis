# Discord Bot Token (get from https://discord.com/developers/applications)
DISCORD_TOKEN=your_discord_bot_token_here

# OpenRouter API Key (used by Tartarus backend)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# AI Model Configuration (used by Tartarus backend)
AI_MODEL=openai/gpt-4o-mini

# Embedding Model (used by Tartarus backend, optional)
EMBEDDING_MODEL=openai/text-embedding-3-small

# PostgreSQL Configuration (used by Tartarus backend)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=aigis
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Tartarus Backend Configuration
ADMIN_API_KEY=your_admin_api_key_here

# Discord Bot -> Tartarus Connection
TARTARUS_URL=http://localhost:5000
TARTARUS_API_KEY=your_tartarus_client_api_key_here
